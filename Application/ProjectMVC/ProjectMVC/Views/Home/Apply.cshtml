@model ProjectMVC.Models.TransactionListModels
@{
    ViewBag.Title = "Apply";
}
<h2>@ViewBag.Title</h2>
<h3>@ViewBag.Message</h3>

<div class="row">
    <div class="col-md-8">
        @using (Html.BeginForm("AddAccount", "Home", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
        {
            @Html.AntiForgeryToken()
            <hr />
            <div class="form-group" style="margin-top: 8%; margin-bottom: 8%">
                <div class="col-md-10">
                    <input type="submit" class="btn btn-default btn-lg col-md-10 btn-primary" value="Apply" />
                </div>
            </div>
        }

        @section Scripts {
            @Scripts.Render("~/bundles/jqueryval")
        }
    </div>
</div>